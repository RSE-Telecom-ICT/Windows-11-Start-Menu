<policyDefinitions xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" revision="1.0" schemaVersion="1.0" xmlns="http://schemas.microsoft.com/GroupPolicy/2006/07/PolicyDefinitions">
 <policyNamespaces>
    <target prefix="RSEWindows11StartMenu" namespace="RseTelecom.Windows11StartMenu" />
    <using prefix="mycompany" namespace="RseTelecom.Policies.Common" />
	<using prefix="windows" namespace="Microsoft.Policies.Windows" />
 </policyNamespaces>
 <resources minRequiredRevision="1.0" />
  <categories>
    <category name="CtRSEWindows11StartMenu" displayName="$(string.CtRSEWindows11StartMenu)">
	<parentCategory ref="mycompany:CtRseTelecom" />
	</category>
  </categories>
  <policies>
   
   <policy name="Windows11StartMenuEnable" class="User" displayName="$(string.Windows11StartMenuEnable)" explainText="$(string.Windows11StartMenuEnable_Help)" key="Software\Policies\RseTelecom\LogonMessage" valueName="MessageEnable">
    <parentCategory ref="CtRSEWindows11StartMenu" />
    <supportedOn ref="windows:SUPPORTED_Windows_11_0_NOSERVER" />
	<enabledValue>
        <decimal value="1" />
    </enabledValue>
    <disabledValue>
    <delete />
    </disabledValue>
   </policy>

   <policy name="Windows11StartMenuTitle" class="User" displayName="$(string.Windows11StartMenuTitle)" explainText="$(string.Windows11StartMenuTitle_Help)" presentation="$(presentation.Windows11StartMenuTitle)" key="Software\Policies\RseTelecom\LogonMessage">
    <parentCategory ref="CtRSEWindows11StartMenu" />
     <supportedOn ref="windows:SUPPORTED_Windows_11_0_NOSERVER" />
     <elements>
        <text id="Windows11StartMenuTitle" valueName="MessageTitle" required="true" />
     </elements>
   </policy>
   
    <policy name="Windows11StartMenuPrompt" class="User" displayName="$(string.Windows11StartMenuPrompt)" explainText="$(string.Windows11StartMenuPrompt_Help)" presentation="$(presentation.Windows11StartMenuPrompt)" key="Software\Policies\RseTelecom\LogonMessage">
    <parentCategory ref="CtRSEWindows11StartMenu" />
     <supportedOn ref="windows:SUPPORTED_Windows_11_0_NOSERVER" />
     <elements>
        <text id="Windows11StartMenuPrompt" valueName="MessagePrompt" required="true" />
     </elements>
   </policy>

   <policy name="Windows11StartMenuButtons" class="User" displayName="$(string.Windows11StartMenuButtons)" explainText="$(string.Windows11StartMenuButtons_Help)" presentation="$(presentation.Windows11StartMenuButtons)" key="Software\Policies\RseTelecom\LogonMessage">
      <parentCategory ref="CtRSEWindows11StartMenu" />
      <supportedOn ref="windows:SUPPORTED_Windows_11_0_NOSERVER" />
      <elements>
        <enum id="Windows11StartMenuButtons" valueName="MessageButtons">
          <item displayName="$(string.MessageButtons_OKOnly)">
            <value>
              <decimal value="0" />
            </value>
          </item>
          <item displayName="$(string.MessageButtons_OKCancel)">
            <value>
              <decimal value="1" />
            </value>
          </item>
          <item displayName="$(string.MessageButtons_AbortRetryIgnore)">
            <value>
              <decimal value="2" />
            </value>
          </item>
          <item displayName="$(string.MessageButtons_YesNoCancel)">
            <value>
              <decimal value="3" />
            </value>
          </item>
		  <item displayName="$(string.MessageButtons_YesNo)">
            <value>
              <decimal value="4" />
            </value>
          </item>
		  <item displayName="$(string.MessageButtons_RetryCancel)">
            <value>
              <decimal value="5" />
            </value>
          </item>
        </enum>
      </elements>
    </policy>
	
	 <policy name="Windows11StartMenuAlignment" class="User" displayName="$(string.Windows11StartMenuAlignment)" explainText="$(string.Windows11StartMenuAlignment_Help)" presentation="$(presentation.Windows11StartMenuAlignment)" key="Software\Microsoft\Windows\CurrentVersion\Explorer\Advanced">
      <parentCategory ref="CtRSEWindows11StartMenu" />
      <supportedOn ref="windows:SUPPORTED_Windows_11_0_NOSERVER" />
      <elements>
        <enum id="Windows11StartMenuAlignment" valueName="TaskbarAl">
          <item displayName="$(string.TaskbarAl_Left)">
            <value>
              <decimal value="0" />
            </value>
          </item>
          <item displayName="$(string.TaskbarAl_Center)">
            <value>
              <decimal value="1" />
            </value>
          </item>
        </enum>
      </elements>
    </policy>
	
  </policies>
</policyDefinitions>
